- unless @pages.any?
  .alert.alert-error= t(:no_page_found_for, search: @search.search_text)
- else
  = render @pages
  = render :partial => '/shared/load_more', :locals => { :resources => @pages, :path => pages_path }

  = page_navigation_links( @pages, 'pagination', '&#8592; Previous', 'Next &#8594;' )
